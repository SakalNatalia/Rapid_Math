<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LevelCompleted">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/black"
        android:scaleType="centerCrop"
        tools:context=".YourActivity"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-49dp">

        <ImageView
            android:id="@+id/LevelCompletedRectangle"
            android:layout_width="wrap_content"
            android:layout_height="367dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="84dp"
            android:layout_marginEnd="76dp"
            android:layout_marginBottom="264dp"
            app:srcCompat="@drawable/LevelCompletedRectangle2" />

        <ImageView
            android:id="@+id/LevelCompletedCim"
            android:layout_width="221dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="99dp"
            android:layout_marginEnd="90dp"
            android:layout_marginBottom="527dp"
            app:srcCompat="@drawable/LevelCompletedCim" />

        <ImageView
            android:id="@+id/BonusRectangle"
            android:layout_width="wrap_content"
            android:layout_height="73dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="114dp"
            android:layout_marginEnd="107dp"
            android:layout_marginBottom="417dp"
            app:srcCompat="@drawable/BonusRectangle" />

        <ImageView
            android:id="@+id/Bonus15Text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="228dp"
            android:layout_marginEnd="145dp"
            android:layout_marginBottom="444dp"
            app:srcCompat="@drawable/Bonus15Text" />

        <ImageView
            android:id="@+id/BonusText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="145dp"
            android:layout_marginEnd="200dp"
            android:layout_marginBottom="441dp"
            app:srcCompat="@drawable/BonusText" />

        <ImageView
            android:id="@+id/GoToLevelButton"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="103dp"
            android:layout_marginEnd="97dp"
            android:layout_marginBottom="350dp"
            app:srcCompat="@drawable/GoToLevelButton" />

        <ImageView
            android:id="@+id/GoToLevelText"
            android:layout_width="64dp"
            android:layout_height="26dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="176dp"
            android:layout_marginEnd="171dp"
            android:layout_marginBottom="354dp"
            app:srcCompat="@drawable/GoToLevelText" />

        <ImageView
            android:id="@+id/QuitGameAtCurrentLevelButton"
            android:layout_width="172dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="123dp"
            android:layout_marginEnd="115dp"
            android:layout_marginBottom="301dp"
            app:srcCompat="@drawable/QuitGameAtCurrentLevelButton" />

        <ImageView
            android:id="@+id/QuitGameAtCurrentLevelText"
            android:layout_width="132dp"
            android:layout_height="84dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="143dp"
            android:layout_marginEnd="135dp"
            android:layout_marginBottom="280dp"
            app:srcCompat="@drawable/QuitGameAtCurrentLevelText" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>